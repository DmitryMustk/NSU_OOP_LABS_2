<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pauseMenuRoot" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nsu.dmustakaev.controller.PauseMenuController">
    <children>
        <Button fx:id="continueButton" layoutX="150.0" layoutY="58.0" mnemonicParsing="false" onAction="#continueGame" text="Продолжить" />
        <Button fx:id="exitButton" layoutX="150.0" layoutY="110.0" mnemonicParsing="false" onAction="#exitGame" text="Выйти" />
    </children>
</AnchorPane>
